package fileExplorer;


import java.io.IOException;
import java.net.MalformedURLException;

import io.appium.java_client.android.AndroidDriver;


import org.openqa.selenium.By;
import org.testng.annotations.Listeners;
import org.testng.annotations.Test;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.AfterTest;

import util.BaseAppium;
import util.ScreenshotListener;
@Listeners({ScreenshotListener.class})
public class FileExplorerTest {
	public static AndroidDriver androidDriver;
    @Test
    public void f() throws IOException, InterruptedException {
    	String pcFilePath = "E:\\testProjects\\ZengQiang\\src\\test\\resources\\test.txt";
    	String remotePath = "sdcard/";
    	String a = "adb push " + pcFilePath + " " + remotePath;
    	System.out.println("a : " + a);
    	BaseAppium.run(a);

    }
    @BeforeTest
    public void beforeTest() throws IOException, InterruptedException {
    	androidDriver = BaseAppium.init("com.android.fileexplorer", ".FileExplorerActivity"); 
    	ScreenshotListener.driver = androidDriver;
    }

    @AfterTest
    public void afterTest() {
    	androidDriver.quit();
    }

}
